{
  "name": "fastbookingnative",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean --force && rm -rf android/.gradle android/.idea android/app/build android/build && rm -rf node_modules",
    "start": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "patch": "npx patch-package",
    "postinstall": "patch-package",
    "sb:start": "sb-rn-get-stories && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test:watch": "jest --watchAll",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "lint": "expo lint",
    "storybook-generate": "sb-rn-get-stories"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|react-native|react-native-fast-opencv/.*|mime|native-base|react-native-svg|@rn-primitives)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "resolutions": {
    "lightningcss": "1.27.0",
    "undici": "5.26.3"
  },
  "overrides": {
    "undici": "5.26.3"
  },
  "dependencies": {
    "@expo/metro-config": "^0.19.12",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@storybook/react-native": "^8.5.5",
    "@tanstack/react-query": "^5.67.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^52.0.38",
    "expo-application": "^6.0.2",
    "expo-constants": "^17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-device": "^7.0.2",
    "expo-linking": "^7.0.5",
    "expo-localization": "~16.0.1",
    "expo-router": "^4.0.18",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "^2.0.1",
    "i18next": "^24.2.3",
    "i18next-resources-to-backend": "^1.2.1",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-localize": "^3.4.1",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.2",
    "react-native-worklets-core": "^1.5.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "3.4.1",
    "undici": "^6.18.2",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "react-native-svg": "^15.11.2",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
